@charset "UTF-8";
/* 水平间距 */
/* 水平间距 */
.search-box {
  position: -webkit-sticky;
  position: sticky;
  top: 0;
  z-index: 999;
}
.sugg-list {
  padding: 0 5px;
}
.sugg-list .sugg-item {
  font-style: 12px;
  padding: 13px 0;
  border-bottom: 1px solid #efefef;
  display: flex;
  align-items: center;
  justify-content: center;
}
.sugg-list .sugg-item .goods_name {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  margin-right: 3px;
}
.history-box {
  padding: 0 5px;
}
.history-box .histit {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 40px;
  font-style: 13px;
  border-bottom: 1px solid #c7c7c7;
}
.history-box .hislist {
  display: flex;
  flex-wrap: wrap;
  padding: 5px 0;
}
.history-box .hislist .uni-tag {
  margin-top: 5px;
  margin-right: 5px;
}

